<h1>Products</h1>
<div class="products">
  <% for (let i = 0; i < data.length; i++) { %>
  <div class="product">
    <% let x = `product/${data[i]._id}` %>
    <img class="product-image" src="<%= data[i].image %>"></img>
    <div class="text">

    <h2 class="product-name"><%= data[i].name %></h2>
    <div class="product-desc"><%= data[i].desc %></div>
    <div class="product-stock">In stock: <%= data[i].stock %></div>
    <div>
        <div class="read-div-1"><a href=<%= x %> class="open-1" >View</a></div>
    </div>
</div>
  </div>
  <% } %>
</div>
